      "inversedBy": "product_variants"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "cart_items": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::cart-item.cart-item",
      "mappedBy": "product_variants"
    },
    "specific_name": {
      "type": "string",
      "required": true
    }
  }
